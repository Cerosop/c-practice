vector<int> graph[V] :Adj_list
bool hava_circle = false, visit[V] = false

vold dfs(int n)
    visit[n] = true
    for(auto x : graph[n])
        if(visit[x])    hava_circle = true
        if(hava_circle)     return
        dfs(x)

for(int i = 0; i < V && !hava_circle; i++)
    if(!visit[i])   dfs(i)